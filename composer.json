{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "anhskohbo/no-captcha": "^3.7",
    "arkitecht/laravel-twilio": "^1.7",
    "devrabiul/laravel-toaster-magic": "^1.3",
    "doctrine/dbal": "^4.2",
    "essa/api-tool-kit": "^2.1",
    "evilfreelancer/routeros-api-php": "*",
    "google/apiclient": "^2.18",
    "guzzlehttp/guzzle": "^7.9",
    "iankumu/mpesa": "^1.3",
    "kkomelin/laravel-translatable-string-exporter": "^1.23",
    "konekt/html": "^6.5",
    "lab404/laravel-impersonate": "^1.7",
    "laravel/framework": "^11.0",
    "laravel/jetstream": "^5.3",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.5",
    "league/flysystem-aws-s3-v3": "^3.28",
    "maatwebsite/excel": "^3.1",
    "masbug/flysystem-google-drive-ext": "^2.4",
    "mashape/unirest-php": "^3.0",
    "mews/purifier": "^3.4",
    "milon/barcode": "^12.0",
    "nwidart/laravel-modules": "^11.1",
    "orhanerday/open-ai": "^5.2",
    "phpseclib/phpseclib": "^3.0",
    "power-components/livewire-powergrid": "^6.4",
    "psr/simple-cache": "^3.0",
    "spatie/browsershot": "^5.0",
    "spatie/laravel-backup": "^9.3",
    "spatie/laravel-google-calendar": "^3.8",
    "spatie/laravel-permission": "^6.9",
    "srmklive/paypal": "^3.0",
    "stripe/stripe-php": "^15.7",
    "twilio/sdk": "^7.16",
    "whichbrowser/parser": "^2.1",
    "yajra/laravel-datatables": "^11.0",
    "yajra/laravel-datatables-oracle": "^11.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9",
    "fakerphp/faker": "^1.23",
    "icanhazstring/composer-unused": "^0.9.2",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^10.5",
    "spatie/laravel-ignition": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/",
      "Easebuzz\\": "app/Libraries/Easebuzz/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/Helpers/Helpers.php",
      "app/Helpers/NetworkHelper.php",
      "app/Helpers/PackagesHelper.php",
      "app/Helpers/RadiusHelper.php",
      "app/Helpers/CustomHelper.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
